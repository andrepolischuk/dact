{
  "name": "dact",
  "version": "0.3.0",
  "description": "Simple data container",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "standard && ava",
    "prepublish": "babel index.js --out-dir dist",
    "postpublish": "rm -rf dist && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrepolischuk/dact.git"
  },
  "keywords": [
    "simple",
    "data",
    "state",
    "container"
  ],
  "author": "Andrey Polischuk <me@andrepolischuk.com> (https://andrepolischuk.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrepolischuk/dact/issues"
  },
  "homepage": "https://github.com/andrepolischuk/dact#readme",
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.0.0",
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "standard": "^9.0.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": {
      "babelrc": true,
      "presets": [
        "@ava/stage-4"
      ],
      "plugins": [
        "transform-runtime"
      ]
    }
  }
}
